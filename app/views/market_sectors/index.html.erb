<h1>Listing market_sectors</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Blurb</th>
    <th>Body</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @market_sectors.each do |market_sector| %>
  <tr>
    <td><%= market_sector.name %></td>
    <td><%= market_sector.blurb %></td>
    <td><%= market_sector.body %></td>
    <td><%= link_to 'Show', market_sector %></td>
    <td><%= link_to 'Edit', edit_market_sector_path(market_sector) %></td>
    <td><%= link_to 'Destroy', market_sector, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

